 import datetime\n    12\t\n    13\tclass PaygreeAPITester:\n    14\t    def __init__(self, api_key, secret_key=None, environment='sandbox'):\n    15\t        \&quot;\&quot;\&quot;\n    16\t        Initialise le testeur d'API Paygree\n    17\t        \n    18\t        Args:\n    19\t            api_key (str): Clé API publique (obligatoire)\n    20\t            secret_key (str): Clé secrète (optionnelle)\n    21\t            environment (str): 'sandbox' ou 'production'\n    22\t        \&quot;\&quot;\&quot;\n    23\t        self.api_key = api_key\n    24\t        self.secret_key = secret_key\n    25\t        self.environment = environment\n    26\t        \n    27\t        # URLs de base selon l'environnement\n    28\t        self.base_urls = {\n    29\t            'sandbox': 'https://sandbox-api.paygree.com',\n    30\t            'production': 'https://api.paygree.com'\n    31\t        }\n    32\t        \n    33\t        self.base_url = self.base_urls.get(environment, self.base_urls['sandbox'])\n    34\t        \n    35\t        # Headers par défaut\n    36\t        self.headers = {\n    37\t            'Content-Type': 'application/json',\n    38\t            'Accept': 'application/json',\n    39\t            'Authorization': f'Bearer {self.api_key}' if self.api_key else None\n    40\t        }\n    41\t\n    42\t    def print_header(self, title):\n    43\t        \&quot;\&quot;\&quot;Affiche un en-tête formaté\&quot;\&quot;\&quot;\n    44\t        print(f\&quot;\\n{'='*50}\&quot;)\n    45\t        print(f\&quot; {title}\&quot;)\n    46\t        print(f\&quot;{'='*50}\&quot;)\n    47\t\n    48\t    def print_result(self, test_name, success, message, response_data=None):\n    49\t        \&quot;\&quot;\&quot;Affiche le résultat d'un test\&quot;\&quot;\&quot;\n    50\t        status = \&quot;✅ SUCCÈS\&quot; if success else \&quot;❌ ÉCHEC\&quot;\n    51\t        print(f\&quot;\\n{test_name}: {status}\&quot;)\n    52\t        print(f\&quot;Message: {message}\&quot;)\n 