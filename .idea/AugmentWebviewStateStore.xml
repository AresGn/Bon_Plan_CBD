naturel'\\n  },\\n  {\\n    name: 'Cristaux CBD',\\n    slug: 'cristaux-cbd',\\n    description: 'Cristaux de CBD pur \\u00e0 99%, isolat de cannabidiol de haute puret\\u00e9'\\n  },\\n  {\\n    name: 'Accessoires',\\n    slug: 'accessoires',\\n    description: 'Vaporisateurs, grinders et accessoires pour consommer le CBD'\\n  }\\n];\\n\\n// Fonction utilitaire pour faire des requ\\u00eates HTTP\\nfunction makeRequest(url, options = {}) {\\n  return new Promise((resolve, reject) =&gt; {\\n    const urlObj = new URL(url);\\n    const requestOptions = {\\n      hostname: urlObj.hostname,\\n      port: urlObj.port || 3000,\\n      path: urlObj.pathname + urlObj.search,\\n      method: options.method || 'GET',\\n      headers: {\\n        'Content-Type': 'application/json',\\n        ...options.headers\\n      }\\n    };\\n\\n    const req = http.request(requestOptions, (res) =&gt; {\\n      let data = '';\\n      res.on('data', chunk =&gt; data += chunk);\\n      res.on('end', () =&gt; {\\n        try {\\n          const jsonData = data ? JSON.parse(data) : {};\\n          resolve({ status: res.statusCode, data: jsonData, headers: res.headers });\\n        } catch (e) {\\n          resolve({ status: res.statusCode, data: data, headers: res.headers });\\n        }\\n      });\\n    });\\n\\n    req.on('error', reject);\\n    \\n    if (options.body) {\\n      req.write(JSON.stringify(options.body));\\n    }\\n    \\n    req.end();\\n  });\\n}\\n\\n// Connexion admin\\nasync function login() {\\n  console.log('\\ud83d\\udd10 Connexion admin...');\\n  \\n  try {\\n    const response = await makeRequest(`${BASE_URL}/api/auth/login`, {\\n      method: 'POST',\\n      body: {\\n        email: ADMIN_EMAIL,\\n        password: ADMIN_PASSWORD\\n      }\\n    });\\n\\n    if (response.status === 200 &amp;&amp; response.data.token) {\\n      adminToken = response.data.token;\\n      console.log('\\u2705 Connexion r\\u00e9ussie');\\n      return true;\\n    } else {\\n      console.log('\\u274c \\u00c9chec de la connexion:', response.data);\\n      return false;\\n    }\\n  } catch (error) {\\n    console.log('\\u274c Erreur de connexion:', error.message);\\n    return false;\\n  }\\n}\\n\\n// Cr\\u00e9er les cat\\u00e9gories CBD\\nasync function createCBDCategories() {\\n  console.log('\\\\n\\ud83c\\udf3f Cr\\u00e9ation des cat\\u00e9gories CBD professionnelles...\\\\n');\\n  \\n  // V\\u00e9rifier les cat\\u00e9gories existantes\\n  const existingResponse = await makeRequest(`${BASE_URL}/api/categories`);\\n  const existingCategories = existingResponse.data || [];\\n  \\n  console.log(`\\ud83d\\udcca ${existingCategories.length} cat\\u00e9gories existantes:`);\\n  existingCategories.forEach(cat =&gt; {\\n    console.log(`   - ${cat.name} (${cat.slug})`);\\n  });\\n  \\n  console.log('\\\\n\\ud83d\\udd27 Cr\\u00e9ation des nouvelles cat\\u00e9gories...');\\n  \\n  let createdCount = 0;\\n  let skippedCount = 0;\\n  \\n  for (const category of cbdCategories) {\\n    // V\\u00e9rifier si la cat\\u00e9gorie existe d\\u00e9j\\u00e0\\n    const exists = existingCategories.find(cat =&gt; cat.slug === category.slug);\\n    \\n    if (exists) {\\n      console.log(`\\u23ed\\ufe0f  ${category.name} existe d\\u00e9j\\u00e0`);\\n      skippedCount++;\\n      continue;\\n    }\\n    \\n    try {\\n      const response = await makeRequest(`${BASE_URL}/api/admin/categories`, {\\n        method: 'POST',\\n        headers: {\\n          'Authorization': `Bearer ${adminToken}`\\n        },\\n        body: {\\n          ...category,\\n          createdAt: new Date().toISOString(),\\n          updatedAt: new Date().toISOString()\\n        }\\n      });\\n      \\n      if (response.status === 201 || response.status === 200) {\\n        console.log(`\\u2705 ${category.name} cr\\u00e9\\u00e9e avec succ\\u00e8s`);\\n        createdCount++;\\n      } else {\\n        console.log(`\\u274c \\u00c9chec pour ${category.name}: ${response.status} - ${JSON.stringify(response.data)}`);\\n      }\\n    } catch (error) {\\n      console.log(`\\u274c Erreur pour ${category.name}:`, error.message);\\n    }\\n    \\n    // Petite pause entre les cr\\u00e9ations\\n    await new Promise(resolve =&gt; setTimeout(resolve, 200));\\n  }\\n  \\n  console.log(`\\\\n\\ud83d\\udcc8 R\\u00e9sum\\u00e9: ${createdCount} cr\\u00e9\\u00e9es, ${skippedCount} existantes`);\\n  \\n  // V\\u00e9rifier le r\\u00e9sultat final\\n  const finalResponse = await makeRequest(`${BASE_URL}/api/categories`);\\n  const finalCategories = finalResponse.data || [];\\n  \\n  console.log(`\\\\n\\ud83c\\udfaf R\\u00c9SULTAT FINAL: ${finalCategories.length} cat\\u00e9gories disponibles:`);\\n  finalCategories.forEach((cat, index) =&gt; {\\n    console.log(`   ${index + 1}. ${cat.name}`);\\n    console.log(`      Slug: ${cat.slug}`);\\n    console.log(`      ID: ${cat.id}`);\\n    console.log('');\\n  });\\n  \\n  return finalCategories;\\n}\\n\\n// Fonction principale\\nasync function main() {\\n  console.log('\\ud83c\\udfea CR\\u00c9ATION DES CAT\\u00c9GORIES CBD PROFESSIONNELLES\\\\n');\\n  \\n  // Attendre que le serveur soit pr\\u00eat\\n  await new Promise(resolve =&gt; setTimeout(resolve, 2000));\\n  \\n  // Connexion\\n  const loginSuccess = await login();\\n  if (!loginSuccess) {\\n    console.log('\\u274c Impossible de continuer sans connexion admin');\\n    return;\\n  }\\n  \\n  // Cr\\u00e9er les cat\\u00e9gories\\n  const categories = await createCBDCategories();\\n  \\n  console.log('\\\\n\\ud83c\\udf89 TERMIN\\u00c9 !');\\n  console.log('\\\\n\\ud83d\\udccb INSTRUCTIONS POUR L\\\\'UTILISATEUR:');\\n  console.log('1. Actualisez la page de cr\\u00e9ation de produit (F5)');\\n  console.log(`2. Vous devriez maintenant voir ${categories.length} cat\\u00e9gories dans le s\\u00e9lecteur`);\\n  console.log('3. Choisissez la cat\\u00e9gorie appropri\\u00e9e pour votre produit');\\n  console.log('4. Remplissez les autres champs et cr\\u00e9ez votre produit');\\n  \\n 