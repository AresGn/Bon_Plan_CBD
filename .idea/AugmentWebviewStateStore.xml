c(filePath).size / 1024;\n    45\t    \n    46\t    try {\n    47\t        if (ext === '.jpg' || ext === '.jpeg') {\n    48\t            await sharp(filePath)\n    49\t                .jpeg({ quality: 85, mozjpeg: true })\n    50\t                .toFile(filePath + '.tmp');\n    51\t        } else if (ext === '.png') {\n    52\t            await sharp(filePath)\n    53\t                .png({ quality: 90, compressionLevel: 9 })\n    54\t                .toFile(filePath + '.tmp');\n    55\t        } else {\n    56\t            return null;\n    57\t        }\n    58\t        \n    59\t        // Remplacer l'original\n    60\t        fs.unlinkSync(filePath);\n    61\t        fs.renameSync(filePath + '.tmp', filePath);\n    62\t        \n    63\t        const newSize = fs.statSync(filePath).size / 1024;\n    64\t        const reduction = ((originalSize - newSize) / originalSize) * 100;\n    65\t        \n    66\t        return {\n    67\t            name: fileName,\n    68\t            originalSize: originalSize.toFixed(2),\n    69\t            newSize: newSize.toFixed(2),\n    70\t            reduction: reduction.toFixed(2)\n    71\t        };\n    72\t    } catch (error) {\n    73\t        console.error(`Erreur avec ${fileName}:`, error.message);\n    74\t        return null;\n    75\t    }\n    76\t}\n...\nPath: .gitignore\n     1\t# Node.js\n     2\tnode_modules/\n     3\t\n     4\t# Next.js\n     5\t.next/\n     6\tout/\n     7\t\n     8\t# Build directories\n     9\tbuild-cpanel/\n    10\t\n    11\t# Logs\n    12\tnpm-debug.log*\n    13\tyarn-debug.log*\n    14\tyarn-error.log*\n    15\t\n    16\t# OS\n    17\t.DS_Store\n    18\tThumbs.db\n    19\t\n    20\t# Environment files (all files starting with .env)\n    21\t.env*\n    22\t*env*\n    23\t\n    24\t# ZIP files\n    25\t*.zip\n    26\t\n    27\t# Others\n    28\t*.log\n    29\t\n    30\t# Uploads\n    31\tpublic/uploads/\n    32\t\n    33\t# Database\n    34\tprisma/dev.db\n    35\tprisma/dev.db-journal\n  