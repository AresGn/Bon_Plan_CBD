: {\\n        headers: {\\n          'Authorization': `Bearer ${process.env.PAYGREEN_SECRET_KEY}`,\\n          'Content-Type': 'application/json'\\n        }\\n      }\\n    },\\n    {\\n      name: 'Direct Secret Key',\\n      config: {\\n        headers: {\\n          'Authorization': process.env.PAYGREEN_SECRET_KEY,\\n          'Content-Type': 'application/json'\\n        }\\n      }\\n    },\\n    {\\n      name: 'Basic Auth (secret:empty)',\\n      config: {\\n        auth: {\\n          username: process.env.PAYGREEN_SECRET_KEY,\\n          password: ''\\n        },\\n        headers: {\\n          'Content-Type': 'application/json'\\n        }\\n      }\\n    },\\n    {\\n      name: 'Basic Auth (shop:secret)',\\n      config: {\\n        auth: {\\n          username: process.env.PAYGREEN_SHOP_ID,\\n          password: process.env.PAYGREEN_SECRET_KEY\\n        },\\n        headers: {\\n          'Content-Type': 'application/json'\\n        }\\n      }\\n    },\\n    {\\n      name: 'API Key Header',\\n      config: {\\n        headers: {\\n          'X-API-Key': process.env.PAYGREEN_SECRET_KEY,\\n          'Content-Type': 'application/json'\\n        }\\n      }\\n    },\\n    {\\n      name: 'PayGreen-Authorization',\\n      config: {\\n        headers: {\\n          'PayGreen-Authorization': process.env.PAYGREEN_SECRET_KEY,\\n          'Content-Type': 'application/json'\\n        }\\n      }\\n    }\\n  ];\\n\\n  for (const format of formats) {\\n    console.log(`\\ud83d\\udd04 Test: ${format.name}...`);\\n    \\n    try {\\n      const response = await axios.post(\\n        `${baseURL}/payment/payment-orders`,\\n        testOrder,\\n        format.config\\n      );\\n      \\n      console.log(`\\u2705 ${format.name} - SUCC\\u00c8S !`);\\n      console.log(`   Status: ${response.status}`);\\n      console.log(`   Payment Order ID: ${response.data.id}`);\\n      console.log(`   Redirect URL: ${response.data.redirect_url}\\\\n`);\\n      \\n      // Si on tr