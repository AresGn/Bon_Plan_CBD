 .from('Product')\\n      .select(`\\n        id,\\n        name,\\n        categoryId,\\n        price,\\n        stock,\\n        category:Category(name)\\n      `)\\n      .order('createdAt', { ascending: false })\\n      .limit(10)\\n    \\n    if (prodError) {\\n      console.log('\\u274c Erreur lors de la r\\u00e9cup\\u00e9ration des produits:', prodError)\\n    } else {\\n      console.log(`\\u2705 ${products.length} produits trouv\\u00e9s (10 derniers):`)\\n      products.forEach(prod =&gt; {\\n        console.log(`   - ${prod.name} (ID: ${prod.id})`)\\n        console.log(`     Cat\\u00e9gorie ID: ${prod.categoryId}`)\\n        console.log(`     Cat\\u00e9gorie: ${prod.category?.name || 'NON TROUV\\u00c9E'}`)\\n        console.log(`     Prix: ${prod.price}\\u20ac, Stock: ${prod.stock}`)\\n        console.log('')\\n      })\\n    }\\n    \\n    // 3. V\\u00e9rifier les utilisateurs admin\\n    console.log('\\ud83d\\udc64 V\\u00e9rification des utilisateurs admin:')\\n    const { data: admins, error: adminError } = await supabase\\n      .from('User')\\n      .select('id, email, role, name')\\n      .eq('role', 'ADMIN')\\n    \\n    if (adminError) {\\n      console.log('\\u274c Erreur lors de la r\\u00e9cup\\u00e9ration des admins:', adminError)\\n    } else {\\n      console.log(`\\u2705 ${admins.length} administrateurs trouv\\u00e9s:`)\\n      admins.forEach(admin =&gt; {\\n        console.log(`   - ${admin.email} (${admin.name || 'Sans nom'})`)\\n      })\\n    }\\n    \\n    console.log('')\\n    \\n    // 4. Cr\\u00e9er des cat\\u00e9gories par d\\u00e9faut si aucune n'existe\\n    if (categories.length === 0) {\\n      console.log('\\ud83d\\udd27 Aucune cat\\u00e9gorie trouv\\u00e9e, cr\\u00e9ation des cat\\u00e9gories par d\\u00e9faut...')\\n      \\n      const defaultCategories = [\\n        {\\n          name: 'Fleurs CBD',\\n          slug: 'fleurs-cbd',\\n          description: 'Fleurs de cannabis CBD de haute qualit\\u00e9'\\n        },\\n        {\\n          name: 'Huiles CBD',\\n          slug: 'huiles-cbd',\\n          description: 'Huiles et extraits de CBD'\\n        },\\n        {\\n          name: 'R\\u00e9sines CBD',\\n          slug: 'resines-cbd',\\n          description: 'R\\u00e9sines et hash CBD'\\n        }\\n      ]\\n      \\n      for (const category of defaultCategories) {\\n        const { data, error } = await supabase\\n          .from('Category')\\n          .insert({\\n            ...category,\\n            createdAt: new Date().toISOString(),\\n            updatedAt: new Date().toISOString()\\n          })\\n          .select()\\n          .single()\\n        \\n        if (error) {\\n          console.log(`\\u274c Erreur lors de la cr\\u00e9ation de ${category.name}:`, error)\\n        } else {\\n          console.log(`\\u2705 Cat\\u00e9gorie ${category.name} cr\\u00e9\\u00e9e avec l'ID: ${data.id}`)\\n        }\\n      }\\n    }\\n    \\n    // 5. Test de cr\\u00e9ation d'un produit simple\\n    if (categories.length &gt; 0) {\\n      console.log('\\ud83e\\uddea Test de cr\\u00e9ation d\\\\'un produit...')\\n      \\n      const testProduct = {\\n        name: 'Test Product Database Check',\\n        slug: 'test-product-database-check',\\n        description: 'Produit de test pour v\\u00e9rifier la base de donn\\u00e9es',\\n        price: 15.99,\\n        originalPrice: 0,\\n        stock: 10,\\n        images: ['https://via.placeholder.com/400x300?text=Test'],\\n        cbdRate: 12.5,\\n        thcRate: 0.2,\\n        origin: 'France',\\n        cultivationType: 'indoor',\\n        terpenes: ['Myrc\\u00e8ne'],\\n        effects: ['Relaxant'],\\n        labAnalysis: '',\\n        status: 'ACTIVE',\\n        featured: false,\\n        categoryId: categories[0].id,\\n        createdAt: new Date().toISOString(),\\n        updatedAt: new Date().toISOString()\\n      }\\n      \\n      const { data: newProduct, error: createError } = await supabase\\n        .from('Product')\\n       