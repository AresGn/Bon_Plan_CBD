 status: res.statusCode, data: data, headers: res.headers });\\n        }\\n      });\\n    });\\n\\n    req.on('error', reject);\\n    \\n    if (options.body) {\\n      req.write(JSON.stringify(options.body));\\n    }\\n    \\n    req.end();\\n  });\\n}\\n\\n// Test de connexion admin\\nasync function testAdminLogin() {\\n  console.log('\\ud83d\\udd10 Connexion admin...');\\n  \\n  try {\\n    const response = await makeRequest(`${BASE_URL}/api/auth/login`, {\\n      method: 'POST',\\n      body: {\\n        email: ADMIN_EMAIL,\\n        password: ADMIN_PASSWORD\\n      }\\n    });\\n\\n    if (response.status === 200 &amp;&amp; response.data.token) {\\n      adminToken = response.data.token;\\n      console.log('\\u2705 Connexion r\\u00e9ussie');\\n      return true;\\n    } else {\\n      console.log('\\u274c \\u00c9chec de la connexion:', response.data);\\n      return false;\\n    }\\n  } catch (error) {\\n    console.log('\\u274c Erreur de connexion:', error.message);\\n    return false;\\n  }\\n}\\n\\n// Test avec diff\\u00e9rents types de donn\\u00e9es pour identifier le probl\\u00e8me\\nasync function debugValidation() {\\n  console.log('\\\\n\\ud83d\\udd0d DEBUG - Test de validation avec diff\\u00e9rents cas...\\\\n');\\n  \\n  // R\\u00e9cup\\u00e9rer les cat\\u00e9gories\\n  const categoriesResponse = await makeRequest(`${BASE_URL}/api/categories`);\\n  if (categoriesResponse.status !== 200 || categoriesResponse.data.length === 0) {\\n    console.log('\\u274c Aucune cat\\u00e9gorie disponible');\\n    return;\\n  }\\n  \\n  const categoryId = categoriesResponse.data[0].id;\\n  console.log(`\\ud83d\\udcc2 Cat\\u00e9gorie disponible: ${categoriesResponse.data[0].name} (${categoryId})\\\\n`);\\n\\n  // Test 1: Donn\\u00e9es minimales valides\\n  console.log('1\\ufe0f\\u20e3 Test avec donn\\u00e9es minimales valides:');\\n  const minimalData = {\\n    name: 'Test Minimal',\\n    categoryId: categoryId,\\n    price: 10.99,\\n    stock: 5,\\n    cbdRate: 10.0