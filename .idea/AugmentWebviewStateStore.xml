e connexion admin\\nasync function testAdminLogin() {\\n  console.log('\\ud83d\\udd10 Test de connexion admin...');\\n  \\n  try {\\n    const response = await makeRequest(`${BASE_URL}/api/auth/login`, {\\n      method: 'POST',\\n      body: {\\n        email: ADMIN_EMAIL,\\n        password: ADMIN_PASSWORD\\n      }\\n    });\\n\\n    if (response.status === 200 &amp;&amp; response.data.token) {\\n      adminToken = response.data.token;\\n      console.log('\\u2705 Connexion admin r\\u00e9ussie');\\n      return true;\\n    } else {\\n      console.log('\\u274c \\u00c9chec de la connexion admin:', response.data);\\n      return false;\\n    }\\n  } catch (error) {\\n    console.log('\\u274c Erreur lors de la connexion admin:', error.message);\\n    return false;\\n  }\\n}\\n\\n// Test des deux endpoints\\nasync function testBothEndpoints() {\\n  console.log('\\ud83e\\uddea Test des deux endpoints de cr\\u00e9ation de produit...\\\\n');\\n  \\n  // R\\u00e9cup\\u00e9rer les cat\\u00e9gories\\n  const categoriesResponse = await makeRequest(`${BASE_URL}/api/categories`);\\n  if (categoriesResponse.status !== 200 || categoriesResponse.data.length === 0) {\\n    console.log('\\u274c Aucune cat\\u00e9gorie disponible');\\n    return;\\n  }\\n  \\n  const categoryId = categoriesResponse.data[0].id;\\n  console.log(`\\ud83d\\udcc2 Utilisation de la cat\\u00e9gorie: ${categoriesResponse.data[0].name} (${categoryId})`);\\n  \\n  const testProduct = {\\n    name: 'Test Final Product',\\n    categoryId: categoryId,\\n    price: 25.99,\\n    originalPrice: 29.99,\\n    stock: 30,\\n    cbdRate: 18.5,\\n    thcRate: 0.15,\\n    status: 'ACTIVE',\\n    description: 'Produit de test final pour v\\u00e9rifier les corrections',\\n    images: ['https://via.placeholder.com/400x300?text=Test+Final'],\\n    terpenes: ['Pin\\u00e8ne', 'Linalol'],\\n    effects: ['\\u00c9nergisant', 'Focus'],\\n    featured: false,\\n    origin: 'Suisse',\\n    cultivationType: 'outdoor'\\n  };\\n\\n  // Test 1: Endpoint /api/admin/products\\n  console.log('1\\ufe0f\\u20e3 Test de /api/admin/products:');\\n  try {\\n    const response1 = await makeRequest(`${BASE_URL}/api/admin/products`, {\\n      method: 'POST',\\n      headers: {\\n        'Authorization': `Bearer ${adminToken}`\\n      },\\n      body: testProduct\\n    });\\n\\n    if (response1.status === 201 || response1.status === 200) {\\n      console.log('\\u2705 /api/admin/products - Succ\\u00e8s');\\n      console.log(`   ID: ${response1.data.id}`);\\n      \\n      // Supprimer le produit de test\\n      await makeRequest(`${BASE_URL}/api/admin/products?id=${response1.data.id}`, {\\n        method: 'DELETE',\\n        headers: {\\n          'Authorization': `Bearer ${adminToken}`\\n        }\\n      });\\n      console.log('\\ud83d\\uddd1\\ufe0f Produit supprim\\u00e9');\\n    } else {\\n      console.log('\\u274c /api/admin/products - \\u00c9chec');\\n      console.log(`   Status: ${response1.status}`);\\n      console.log(`   Erreur: ${JSON.stringify(response1.data, null, 2)}`);\\n    }\\n  } catch (error) {\\n    console.log('\\u274c /api/admin/products - Erreur:', error.message);\\n  }\\n\\n  console.log('');\\n\\n  // Test 2: Endpoint /api/products\\n  console.log('2\\ufe0f\\u20e3 Test de /api/products:');\\n  try {\\n    const testProduct2 = { ...testProduct, name: 'Test Final Product 2' };\\n    const response2 = await makeRequest(`${BASE_URL}/api/products`, {\\n      method: 'POST',\\n      headers: {\\n        'Authorization': `Bearer ${adminToken}`\\n      },\\n      body: testProduct2\\n    });\\n\\n    if (response2.status === 201 || response2.status === 200) {\\n      console.log('\\u2705 /api/products - Succ\\u00e8s');\\n      console.log(`   ID: ${response2.data.id}`);\\n      \\n      // Supprimer le produit de test via l'API admin\\n      await makeRequest(`${BASE_URL}/api/admin/products?id=${response2.data.id}`, {\\n        method: 'DELETE',\\n        headers: {\\n          'Authorization': `Bearer ${adminToken}`\\n        }\\n      });\\n      console.log('\\ud83d\\uddd1\\ufe0f Produit supprim\\u00e9');\\n    } else {\\n      console.log('\\u274c /api/products - \\u00c9chec');\\n      console.log(`   Status: ${response2.status}`);\\n      console.log(`   Erreur: ${JSON.stringify(response2.data, null, 2)}`);\\n    }\\n  } catch (error) {\\n    console.log('\\u274c /api/products - Erreur:', error.message);\\n  }\\n}\\n\\n// Test de validation avec donn\\u00e9es invalides\\nasync function testValidation() {\\n  console.log('\\\\n\\ud83d\\udd0d Test de validation avec donn\\u00e9es invalides...');\\n  \\n  const invalidProduct = {\\n    name: '',  // Vide\\n    categoryId: '',  // Vide\\n    price: 'invalid',  // Non num\\u00e9rique\\n    stock: -5,  // N\\u00e9gatif\\n    cbdRate: 'NaN',  // Non num\\u00e9rique\\n    thcRate: 0.5,  // Trop \\u00e9lev\\u00e9\\n    description: 'Test de validation'\\n  };\\n\\n  try {\\n    const response = await makeRequest(`${BASE_URL}/api/products`, {\\n      method: 'POST',\\n      headers: {\\n        'Authorization': `Bearer ${adminToken}`\\n      },\\n      body: invalidProduct\\n    });\\n\\n    if (response.status === 400) {\\n      console.log('\\u2705 Validation fonctionne correctement');\\n      console.log(`   Erreur: ${response.data.error}`);\\n    } else {\\n      console.log('\\u274c La validation devrait \\u00e9chouer');\\n      console.log(`   Status: ${response.status}`);\\n      console.log(`   R\\u00e9ponse: ${JSON.stringify(response.data, null, 2)}`);\\n    }\\n  } catch (error) {\\n    console.log('\\u274c Erreur lors du test de validation:', error.message);\\n  }\\n}\\n\\n// Fonction principale\\nasync function runFinalTests() {\\n  console.log('\\ud83c\\udfaf TESTS FINAUX - V\\u00e9rification des corrections\\\\n');\\n  \\n  // Test de connexion\\n  const loginSuccess = await testAdminLogin();\\n  if (!loginSuccess) {\\n    console.log('\\u274c Impossible de continuer sans connexion admin');