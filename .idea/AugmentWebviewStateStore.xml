rn true;\\n    } else {\\n      console.log('\\u274c \\u00c9chec de la connexion admin:', response.data);\\n      return false;\\n    }\\n  } catch (error) {\\n    console.log('\\u274c Erreur lors de la connexion admin:', error.message);\\n    return false;\\n  }\\n}\\n\\n// Test d'initialisation des cat\\u00e9gories\\nasync function testSeedCategories() {\\n  console.log('\\ud83d\\udcc2 Test d\\\\'initialisation des cat\\u00e9gories...');\\n  \\n  try {\\n    const response = await makeRequest(`${BASE_URL}/api/admin/seed-categories`, {\\n      method: 'POST',\\n      headers: {\\n        'Authorization': `Bearer ${adminToken}`\\n      }\\n    });\\n\\n    if (response.status === 200) {\\n      console.log('\\u2705 Cat\\u00e9gories initialis\\u00e9es avec succ\\u00e8s');\\n      console.log(`\\ud83d\\udcca ${response.data.categories.length} cat\\u00e9gories disponibles`);\\n      return response.data.categories;\\n    } else {\\n      console.log('\\u274c \\u00c9chec de l\\\\'initialisation des cat\\u00e9gories:', response.data);\\n      return null;\\n    }\\n  } catch (error) {\\n    console.log('\\u274c Erreur lors de l\\\\'initialisation des cat\\u00e9gories:', error.message);\\n    return null;\\n  }\\n}\\n\\n// Test de r\\u00e9cup\\u00e9ration des cat\\u00e9gories\\nasync function testGetCategories() {\\n  console.log('\\ud83d\\udccb Test de r\\u00e9cup\\u00e9ration des cat\\u00e9gories...');\\n  \\n  try {\\n    const response = await makeRequest(`${BASE_URL}/api/categories`);\\n\\n    if (response.status === 200 &amp;&amp; response.data.length &gt; 0) {\\n      console.log('\\u2705 Cat\\u00e9gories r\\u00e9cup\\u00e9r\\u00e9es avec succ\\u00e8s');\\n      console.log(`\\ud83d\\udcca ${response.data.length} cat\\u00e9gories trouv\\u00e9es`);\\n      return response.data;\\n    } else {\\n      console.log('\\u274c Aucune cat\\u00e9gorie trouv\\u00e9e');\\n      return [];\\n    }\\n  } catch (error) {\\n    console.log('\\u274c Erreur lors de la r\\u00e9cup\\