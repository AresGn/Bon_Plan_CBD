t; data += chunk);\\n      res.on('end', () =&gt; {\\n        try {\\n          const jsonData = data ? JSON.parse(data) : {};\\n          resolve({ status: res.statusCode, data: jsonData, headers: res.headers });\\n        } catch (e) {\\n          resolve({ status: res.statusCode, data: data, headers: res.headers });\\n        }\\n      });\\n    });\\n\\n    req.on('error', reject);\\n    \\n    if (options.body) {\\n      req.write(JSON.stringify(options.body));\\n    }\\n    \\n    req.end();\\n  });\\n}\\n\\nasync function testCategories() {\\n  console.log('\\ud83d\\udd0d Test des cat\\u00e9gories disponibles...\\\\n');\\n  \\n  try {\\n    // Test de l'endpoint des cat\\u00e9gories\\n    console.log('1\\ufe0f\\u20e3 Test de l\\\\'endpoint /api/categories:');\\n    const response = await makeRequest(`${BASE_URL}/api/categories`);\\n    \\n    if (response.status === 200) {\\n      console.log(`\\u2705 Endpoint accessible (Status: ${response.status})`);\\n      console.log(`\\ud83d\\udcca ${response.data.length} cat\\u00e9gories trouv\\u00e9es:`);\\n      \\n      if (response.data.length === 0) {\\n        console.log('\\u274c PROBL\\u00c8ME: Aucune cat\\u00e9gorie disponible !');\\n        console.log('   L\\\\'interface ne peut pas fonctionner sans cat\\u00e9gories.');\\n        \\n        // Essayer de cr\\u00e9er des cat\\u00e9gories par d\\u00e9faut\\n        console.log('\\\\n\\ud83d\\udd27 Tentative de cr\\u00e9ation de cat\\u00e9gories par d\\u00e9faut...');\\n        await createDefaultCategories();\\n        \\n      } else {\\n        response.data.forEach((cat, index) =&gt; {\\n          console.log(`   ${index + 1}. ${cat.name} (ID: ${cat.id})`);\\n          console.log(`      Slug: ${cat.slug}`);\\n          console.log(`      Description: ${cat.description || 'Aucune'}`);\\n        });\\n        \\n        console.log('\\\\n\\u2705 Les cat\\u00e9gories sont disponibles pour l\\\\'interface !');\\n      }\\n    } else {\\n      console.log(`\\u274c Erreur d\\\\'acc\\u00e8s \\u00e0 l\\\\'endpoint (Status: ${response.status})`);\\n      console.log(`   R\\u00e9ponse: ${JSON.stringify(response.data, null, 2)}`);\\n    }\\n    \\n  } catch (error) {\\n    console.log('\\u274c Erreur lors du test des cat\\u00e9gories:', error.message);\\n  }\\n}\\n\\nasync function createDefaultCategories() {\\n  const defaultCategories = [\\n    {\\n      name: 'Fleurs CBD',\\n      slug: 'fleurs-cbd',\\n      description: 'Fleurs de cannabis CBD de haute qualit\\u00e9'\\n    },\\n    {\\n      name: 'Huiles CBD',\\n      slug: 'huiles-cbd', \\n      description: 'Huiles et extraits de CBD'\\n    },\\n    {\\n      name: 'R\\u00e9sines CBD',\\n      slug: 'resines-cbd',\\n      description: 'R\\u00e9sines et hash CBD'\\n    }\\n  ];\\n\\n  try {\\n    for (const category of defaultCategories) {\\n      console.log(`   Cr\\u00e9ation de: ${category.name}...`);\\n      \\n      const response = await makeRequest(`${BASE_URL}/api/categories`, {\\n        method: 'POST',\\n        body: {\\n          ...category,\\n          createdAt: new Date().toISOString(),\\n          updatedAt: new Date().toISOString()\\n        }\\n      });\\n      \\n      if (response.status === 201 || response.status === 200) {\\n        console.log(`   \\u2705 ${category.name} cr\\u00e9\\u00e9e avec succ\\u00e8s`);\\n      } else {\\n        console.log(`   \\u274c \\u00c9chec de cr\\u00e9ation de ${category.name}: ${response.status}`);\\n      }\\n    }\\n    \\n    // V\\u00e9rifier \\u00e0 nouveau les cat\\u00e9gories\\n    console.log('\\\\n\\ud83d\\udd04 V\\u00e9rification apr\\u00e8s cr\\u00e9ation...');\\n    const checkResponse = await makeRequest(`${BASE_URL}/api/categories`);\\n    if (checkResponse.status === 200 &amp;&amp; checkResponse.data.length &gt; 0) {\\n      console.log(`\\u2705 ${checkResponse.data.length} cat\\u00e9gories maintenant disponibles !`);\\n      checkResponse.data.forEach((cat, index) =&gt; {\\n        console.log(`   ${index + 1}. ${cat.name} (ID: ${cat.id})`);\\n      });\\n    }\\n    \\n  } catch (error) {\\n    console.log('\\u274c Erreur lors de la cr\\u00e9ation des cat\\u00e9gories:', error.message);\\n  }\\n}\\n\\nasync function testCategorySelection() {\\n  console.log('\\\\n2\\ufe0f\\u20e3 Test de s\\u00e9lection de cat\\u00e9gorie pour un produit:');\\n  \\n  try {\\n    const categoriesResponse = await makeRequest(`${BASE_URL}/api/categories`);\\n    \\n    if (categoriesResponse.status === 200 &amp;&amp; categoriesResponse.data.length &gt; 0) {\\n      const firstCategory = categoriesResponse.data[0];\\n      console.log(`\\u2705 Cat\\u00e9gorie s\\u00e9lectionn\\u00e9e pour test: ${firstCategory.name}`);\\n      console.log(`   ID: ${firstCategory.id}`);\\n      console.log(`   Cet ID devrait \\u00eatre utilis\\u00e9 dans le formulaire de cr\\u00e9ation de produit`);\\n      \\n      // Simuler les donn\\u00e9es comme l'interface les enverrait avec une cat\\u00e9gorie valide\\n      const testProductData = {\\n        name: 'Test Cat\\u00e9gorie Valide',\\n        categoryId: firstCategory.id, // ID valide\\n        price: 19.99,\\n        stock: 25,\\n        cbdRate: 15.5,\\n        thcRate: 0.2,\\n        description: 'Test avec cat\\u00e9gorie valide'\\n      };\\n      \\n      console.log('\\\\n\\ud83d\\udcdd Donn\\u00e9es de test avec cat\\u00e9gorie valide:');\\n      console.log(JSON.stringify(testProductData, null, 2));\\n      \\n      return firstCategory.id;\\n    } else {\\n      console.log('\\u274c Aucune cat\\u00e9gorie disponible pour le test');\\n      return null;\\n    }\\n  } catch (error) {\\n    console.log('\\u274c Erreur lors du test de s\\u00e9lection:', error.message);\\n    return null;\\n  }\\n}\\n\\nasync function main() {\\n  console.log('\\ud83e\\uddea DIAGNOSTIC DES CAT\\u00c9GORIES POUR L\\\\'INTERFACE ADMIN\\\\n');\\n  \\n  // Attendre que le serveur soit pr\\u00eat\\n  await new Promise(resolve =&gt; setTimeout(resolve, 2000));\