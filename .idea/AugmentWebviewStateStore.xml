port || 3000,\\n      path: urlObj.pathname + urlObj.search,\\n      method: options.method || 'GET',\\n      headers: {\\n        'Content-Type': 'application/json',\\n        ...options.headers\\n      }\\n    };\\n\\n    const req = http.request(requestOptions, (res) =&gt; {\\n      let data = '';\\n      res.on('data', chunk =&gt; data += chunk);\\n      res.on('end', () =&gt; {\\n        try {\\n          const jsonData = data ? JSON.parse(data) : {};\\n          resolve({ status: res.statusCode, data: jsonData, headers: res.headers });\\n        } catch (e) {\\n          resolve({ status: res.statusCode, data: data, headers: res.headers });\\n        }\\n      });\\n    });\\n\\n    req.on('error', reject);\\n    \\n    if (options.body) {\\n      req.write(JSON.stringify(options.body));\\n    }\\n    \\n    req.end();\\n  });\\n}\\n\\n// Test de connexion admin\\nasync function testAdminLogin() {\\n  console.log('\\ud83d\\udd10 Connexion admin...');\\n  \\n  try {\\n    const response = await makeRequest(`${BASE_URL}/api/auth/login`, {\\n      method: 'POST',\\n      body: {\\n        email: ADMIN_EMAIL,\\n        password: ADMIN_PASSWORD\\n      }\\n    });\\n\\n    if (response.status === 200 &amp;&amp; response.data.token) {\\n      adminToken = response.data.token;\\n      console.log('\\u2705 Connexion r\\u00e9ussie');\\n      return true;\\n    } else {\\n      console.log('\\u274c \\u00c9chec de la connexion:', response.data);\\n      return false;\\n    }\\n  } catch (error) {\\n    console.log('\\u274c Erreur de connexion:', error.message);\\n    return false;\\n  }\\n}\\n\\n// Test de cr\\u00e9ation d'un produit comme l'interface le ferait\\nasync function testInterfaceProduct() {\\n  console.log('\\\\n\\ud83d\\udda5\\ufe0f Test de cr\\u00e9ation de produit comme l\\\\'interface...\\\\n');\\n  \\n  // R\\u00e9cup\\u00e9rer les cat\\u00e9gories\\n  const categoriesResponse = await makeRequest(`${BASE_URL}/api/categories`);\\n  if (categoriesResponse.